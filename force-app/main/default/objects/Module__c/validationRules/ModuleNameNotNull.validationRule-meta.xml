<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ModuleNameNotNull</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISNEW(),  ISBLANK(Name) )</errorConditionFormula>
    <errorMessage>Module Name cannot be blank</errorMessage>
</ValidationRule>
